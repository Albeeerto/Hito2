<?xml version="1.0" encoding="UTF-8"?>

<?import java.net.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<VBox alignment="CENTER" prefHeight="225.0" prefWidth="284.0" spacing="20.0" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.empresa.javafx_mongo.SeleccionController">
    <stylesheets>
        <URL value="@style_registro.css" />
    </stylesheets>
    <children>
        <Label text="Elige una opción" textAlignment="CENTER">
            <font>
                <Font name="System Bold" size="24.0" />
            </font>
        </Label>
        <Button fx:id="loginButton" onAction="#handleLoginButtonAction" prefHeight="36.0" prefWidth="102.0" styleClass="button" text="Iniciar Sesión" />
        <Button fx:id="registerButton" onAction="#handleRegisterButtonAction" prefHeight="40.0" prefWidth="97.0" styleClass="button" text="Registrarse" />
    </children>
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
</VBox>
